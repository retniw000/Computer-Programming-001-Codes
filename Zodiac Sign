#include <stdio.h>

int main() {
    int day, month;

    printf("Enter Day of Birth:\n");
    scanf("%d", &day);
    printf("Enter Month of Birth:\n");
    scanf("%d", &month);

    // Zodiac determination
    if (month == 1) {
        if (day <= 19)
            printf("Capricorn\n");
        else if (day >= 20)
            printf("Aquarius\n");
    } else if (month == 2) {
        if (day <= 18)
            printf("Aquarius\n");
        else if (day >= 19)
            printf("Pisces\n");
    } else if (month == 4) { // Example for April
        if (day <= 19)
            printf("Aries\n");
        else if (day >= 20)
            printf("Taurus\n");
    }
    // Additional months go here...

    return 0;
}
